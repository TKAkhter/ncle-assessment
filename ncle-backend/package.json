    "dev": "npm run clean && tsc && concurrently \"tsc --watch\" \"nodemon -q dist/server.js\"",
    "test": "jest",
    "watch-ts": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soelinn/express-ts-starter.git"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.1",
    "helmet": "^4.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.9",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.19",
    "@types/morgan": "^1.9.6",
    "@types/node": "^14.14.14",
    "@types/uuid": "^9.0.5",
    "eslint": "^7.16.0",
    "jest": "^26.6.3",
    "typescript": "^4.1.3"
  }
