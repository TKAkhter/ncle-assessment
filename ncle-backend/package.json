{
  "name": "ts-express",
  "version": "1.0.0",
  "main": "src/server.ts",
  "author": "Soe Tun",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "clean": "rm -rf ./dist",
    "serve": "node dist/server.js",
